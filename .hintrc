{
  "connector": {
    "name": "puppeteer",
    "options": {
      "waitUntil": "networkidle2"
    }
  },
  "extends": [
    "development"
  ],
  "formatters": [
    "summary",
    "html"
  ],
  "hints": {
    "compat-api/css": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/name-role-value": "off",
    "axe/structure": "off",
    "css-prefix-order": "off",
    "image-optimization-cloudinary": "off",
    "performance-budget": [
      "error",
      {
        "resourceTypes": {
          "document": 30,
          "image": 50,
          "script": 120,
          "total": 250
        },
        "resourceSizes": {
          "document": 30,
          "image": 500,
          "script": 300,
          "total": 1600
        }
      }
    ]
  }
}
